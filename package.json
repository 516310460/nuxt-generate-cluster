    "get-port": "^4.0.0",
    "jest": "^23.6.0",
    "jsdom": "^12.0.0",
    "klaw-sync": "^6.0.0",
    "nyc": "^13.0.1",
    "pug": "^2.0.3",
    "pug-plain-loader": "^1.0.0",
    "request": "^2.88.0",
    "request-promise-native": "^1.0.5",
    "rimraf": "^2.6.2",
    "rollup": "^0.66.2",
    "rollup-plugin-babel": "^4.0.3",
